{
  "name": "@helium/recent-helium-transactions-service",
  "version": "0.10.13",
  "description": "Service to track and provide recent Helium transactions",
  "private": true,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "ts-node-dev --respawn --project tsconfig.cjs.json src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node lib/cjs/index.js",
    "poll-blocks": "ts-node-dev --project tsconfig.cjs.json src/blockPoller.ts"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@helium/lazy-distributor-sdk": "^0.10.13",
    "@helium/spl-utils": "^0.10.13",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.91.1",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
