    "@solana/spl-governance": "^0.3.18",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.78.4",
    "@sqds/sdk": "2.0.4",
    "axios": "^1.3.6",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.24",
