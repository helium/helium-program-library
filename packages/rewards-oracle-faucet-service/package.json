{
  "name": "@helium/rewards-oracle-faucet-service",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "version": "0.2.15",
  "description": "Receives requests from Helius webhooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/helium/helium-program-libary"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "import": "./lib/esm/src/index.js",
    "require": "./lib/cjs/index.js",
    "types": "./lib/types/src/index.d.ts"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "build": "tsc -p tsconfig.json",
    "start": "node lib/esm/index.js",
    "dev": "npx ts-node --project tsconfig.cjs.json src/index.ts",
    "seed-db": "npx ts-node --project tsconfig.cjs.json src/seed-db.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1318.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.9.2",
    "pg": "^8.9.0",
    "sequelize": "^6.27.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@types/pg": "^8.6.6",
    "@types/yargs": "^17.0.13",
    "git-format-staged": "^2.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "yarn": "^1.22.18"
  },
  "keywords": [],
  "author": ""
}
