{
  "name": "@helium/idls",
  "version": "0.4.1",
  "description": "Exported idls",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/helium/helium-program-library"
  },
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "./*": "./*"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "clean": "npx shx rm -rf **/tsconfig*.tsbuildinfo && npx shx rm -rf lib && npx shx mkdir -p lib/cjs lib/esm",
    "package": "npx shx mkdir -p lib/cjs lib/esm",
    "prebuild": "npm run clean && npm run package"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@solana/web3.js": "^1.78.4",
    "bn.js": "^5.2.0",
    "borsh": "^0.7.0",
    "bs58": "^4.0.1"
  },
  "devDependencies": {
    "ts-loader": "^9.2.3",
    "typescript": "^4.8.4"
  },
  "gitHead": "5a8bf0b7b88e5934ef8d774e686f7c95804fbb8d"
}
