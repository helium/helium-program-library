{
  "name": "@helium-foundation/bootstrap-cli",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "version": "0.0.1",
  "description": "CLI to bootstrap the network",
  "repository": {
    "type": "git",
    "url": "https://github.com/HeliumFoundation/helium-program-libary"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "import": "./lib/esm/src/index.js",
    "require": "./lib/cjs/index.js",
    "types": "./lib/types/src/index.d.ts"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "clean": "npx shx mkdir -p lib && npx shx rm -rf lib",
    "package": "npx shx mkdir -p lib/cjs lib/esm",
    "prebuild": "npm run clean && npm run package"
  },
  "dependencies": {
    "@helium-foundation/data-credits-sdk": "^0.0.1",
    "@helium-foundation/helium-sub-daos-sdk": "^0.0.1",
    "@helium-foundation/lazy-distributor-sdk": "^0.0.1",
    "@helium-foundation/spl-utils": "^0.0.1",
    "@project-serum/anchor": "^0.24.2",
    "@solana/spl-token": "0.3.1",
    "@solana/web3.js": "^1.63.1",
    "bn.js": "^5.2.0",
    "bs58": "^4.0.1",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.13",
    "git-format-staged": "^2.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.3.4",
    "yarn": "^1.22.18"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
